{
    "name": "titlecase-js",
    "version": "1.2.8",
    "description": "Converts a string to title case with multiple style options, ability to ignore certain words, and handle acronyms",
    "keywords": [
        "titlecase",
        "text transformation",
        "capitalization",
        "formatting"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/danielhaim1/titlecase-js.git"
    },
    "main": "dist/titlecase.js",
    "scripts": {
        "test": "jest",
        "build": "webpack --mode production && terser dist/titlecase.js -o dist/titlecase.min.js",
        "build-docs": "npm run build && cp -R dist/ docs/assets/js",
        "tree": "tree -I 'node_modules'"
    },
    "author": {
        "name": "Daniel Haim",
        "url": "https://github.com/danielhaim1"
    },
    "license": "MIT",
    "devDependencies": {
        "@babel/cli": "^7.21.0",
        "@babel/core": "^7.21.0",
        "@babel/plugin-transform-modules-commonjs": "^7.21.2",
        "@babel/plugin-transform-runtime": "^7.21.0",
        "@babel/preset-env": "^7.20.2",
        "@babel/runtime-corejs3": "^7.21.0",
        "@jest/expect": "^29.5.0",
        "babel-jest": "^29.5.0",
        "babel-loader": "^9.1.2",
        "jest": "^29.5.0",
        "jest-environment-jsdom": "^29.5.0",
        "jest-environment-puppeteer": "^8.0.5",
        "jest-puppeteer": "^8.0.5",
        "puppeteer-core": "^19.8.0",
        "terser-webpack-plugin": "^5.3.7",
        "webpack": "^5.76.3",
        "webpack-cli": "^5.0.1",
        "webpack-node-externals": "^3.0.0"
    }
}
