{
    "name": "titlecase-js",
    "version": "1.1.4",
    "description": "Converts the specified string to title case (except for words that are entirely in uppercase, which are considered to be acronyms).",
    "keywords": [
        "titlecase",
        "string",
        "text",
        "conversion",
        "formatting",
        "APA",
        "APA style",
        "Chicago",
        "Chicago manual of style",
        "capitalization",
        "case",
        "text transformation",
        "text processing",
        "writing",
        "language",
        "text editing"
    ],
    "homepage": "https://github.com/danielhaim1/titlecase-js",
    "bugs": {
        "url": "https://github.com/danielhaim1/titlecase-js/issues"
    },
    "repository": {
        "type": "git",
        "url": "git://github.com/danielhaim1/titlecase-js.git"
    },
    "main": "index.js",
    "scripts": {
        "test": "jest",
        "build": "babel index.js -o dist/titlecase.js",
        "build-min": "babel index.js -o dist/titlecase.min.js --no-comments --minified",
        "dev": "npm run build-dev && npm run test",
        "prepublish": "npm run build && npm run build-min"
    },
    "author": {
        "name": "Daniel Haim",
        "url": "https://github.com/danielhaim1"
    },
    "license": "MIT",
    "dependencies": {
        "jest-environment-jsdom": "^29.5.0",
        "lodash": "^4.17.21"
    },
    "devDependencies": {
        "@babel/cli": "^7.21.0",
        "@babel/core": "^7.21.0",
        "@babel/preset-env": "^7.20.2",
        "@jest/expect": "^29.5.0",
        "babel-jest": "^29.5.0",
        "babel-plugin-transform-remove-console": "^6.9.4",
        "jest": "^29.5.0",
        "jest-environment-puppeteer": "^8.0.5",
        "jest-puppeteer": "^8.0.5"
    }
}
